<h1>Places</h1>

<div class="mb-3">
  <button type="button" class="btn btn-outline-success">
  <a href="/places/new">New Place</a>
  </button>
</div>

<div class="mb-3">
  <ul>
    <% for place in @places %>
      <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
    <% end %>
  </ul>
</div>
